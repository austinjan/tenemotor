{
  "name": "tenemotor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.17.0",
    "babel-plugin-import": "^1.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "rescripts start",
